<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.neefull.fsp.app.mapper.TaskAnnexMapper">

    <insert id="saveAnnex" useGeneratedKeys="true" keyProperty="annexId">
        INSERT INTO t_task_annex (
        project_id,
        user_id,
        annex_desc
        )
        VALUES
        (
        #{taskAnnex.projectId},
        #{taskAnnex.userId},
        #{taskAnnex.annexDesc}
        )
    </insert>

</mapper>
